<h1>The Posts Page</h1>

<% for(var i = 0; i < posts.length; i++){ %>
    <li>
        <%= posts[i].title %> -
            <strong><%= posts[i].author %></strong>
    </li>
    <% } %>

        <!-- use a forEach to loop through, don't have to use [i] -->
        <% posts.forEach(function(posts){%>
            <li>
                <%= posts.title %> -
                    <strong><%= posts.author %></strong>
            </li>
            <%})%>